{
    "swagger": "2.0",
    "info": {
      "title": "KerisWatch API",
      "description": "Description",
      "version": "1.0.0"
    },
    "host": "localhost:3000",
    "basePath": "/api",
    "schemes": [
      "http"
    ],
    "paths": {
      "/events": {
        "get": {
          "description": "Get a list of events",
          "responses": {
            "200": {
              "description": "OK"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events/all": {
        "get": {
          "description": "Get a list of all events",
          "responses": {
            "200": {
              "description": "OK"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events/category/{categoryId}": {
        "get": {
          "description": "Get events by category ID",
          "parameters": [
            {
              "name": "categoryId",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "404": {
              "description": "Not Found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events/{eventId}/related/category/{categoryId}": {
        "get": {
          "description": "Get related events by category ID",
          "parameters": [
            {
              "name": "eventId",
              "in": "path",
              "required": true,
              "type": "string"
            },
            {
              "name": "categoryId",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "404": {
              "description": "Not Found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events/with-users": {
        "get": {
          "description": "Get events with users",
          "responses": {
            "200": {
              "description": "OK"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events/{id}/users": {
        "get": {
          "description": "Get number of users joined event",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "404": {
              "description": "Not Found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        },
        "post": {
          "description": "Add user to event",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            },
            {
              "name": "body",
              "in": "body",
              "schema": {
                "type": "object",
                "properties": {
                  "userId": {
                    "example": "any"
                  }
                }
              }
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "400": {
              "description": "Bad Request"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        },
        "delete": {
          "description": "Delete user from event",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "400": {
              "description": "Bad Request"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events/{id}/joined": {
        "get": {
          "description": "Check if user joined event",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "404": {
              "description": "Not Found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/event/{id}": {
        "get": {
          "description": "Get event by ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "404": {
              "description": "Not Found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events/with-users/{eventId}": {
        "get": {
          "description": "Get specific event with users by event ID",
          "parameters": [
            {
              "name": "eventId",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "404": {
              "description": "Not Found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events/create": {
        "post": {
            "description": "Create a new event",
            "consumes": ["multipart/form-data"],
            "parameters": [
            {
                "name": "image",
                "in": "formData",
                "type": "file",
                "required": true,
                "description": "Event image file"
            },
            {
                "name": "name",
                "in": "formData",
                "type": "string",
                "required": true,
                "description": "Event name"
            },
            {
                "name": "description",
                "in": "formData",
                "type": "string",
                "required": true,
                "description": "Event description"
            },
            {
                "name": "categoryId",
                "in": "formData",
                "type": "integer",
                "required": true,
                "description": "Category ID"
            },
            {
                "name": "startDate",
                "in": "formData",
                "type": "string",
                "format": "date-time",
                "required": true,
                "description": "Start date of the event (e.g. 2024-09-27)"
            },
            {
                "name": "endDate",
                "in": "formData",
                "type": "string",
                "format": "date-time",
                "required": true,
                "description": "End date of the event (e.g. 2024-09-28)"
            },
            {
                "name": "locationName",
                "in": "formData",
                "type": "string",
                "required": true,
                "description": "Location name"
            },
            {
                "name": "address",
                "in": "formData",
                "type": "string",
                "required": true,
                "description": "Address"
            },
            {
                "name": "postalCode",
                "in": "formData",
                "type": "string",
                "required": true,
                "description": "Postal code"
            },
            {
                "name": "country",
                "in": "formData",
                "type": "string",
                "required": true,
                "description": "Country"
            },
            {
                "name": "totalCapacity",
                "in": "formData",
                "type": "integer",
                "required": true,
                "description": "Total capacity"
            }
            ],
            "responses": {
            "200": {
                "description": "Event created successfully"
            },
            "400": {
                "description": "Bad Request"
            },
            "500": {
                "description": "Internal Server Error"
            }
           }
        }
    },
      "/events/{id}": {
    "put": {
        "description": "Update an event",
        "consumes": ["multipart/form-data"],
        "parameters": [
        {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "Event ID"
        },
        {
            "name": "image",
            "in": "formData",
            "type": "file",
            "required": false,
            "description": "Event image file"
        },
        {
            "name": "name",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "Event name"
        },
        {
            "name": "description",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "Event description"
        },
        {
            "name": "categoryId",
            "in": "formData",
            "type": "integer",
            "required": true,
            "description": "Category ID"
        },
        {
            "name": "startDate",
            "in": "formData",
            "type": "string",
            "format": "date-time",
            "required": true,
            "description": "Start date of the event (e.g. 2024-09-27)"
        },
        {
            "name": "endDate",
            "in": "formData",
            "type": "string",
            "format": "date-time",
            "required": true,
            "description": "End date of the event (e.g. 2024-09-28)"
        },
        {
            "name": "locationName",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "Location name"
        },
        {
            "name": "address",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "Address"
        },
        {
            "name": "postalCode",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "Postal code"
        },
        {
            "name": "country",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "Country"
        },
        {
            "name": "totalCapacity",
            "in": "formData",
            "type": "integer",
            "required": true,
            "description": "Total capacity"
        }
        ],
        "responses": {
        "200": {
            "description": "Event updated successfully"
        },
        "400": {
            "description": "Bad Request"
        },
        "500": {
            "description": "Internal Server Error"
        }
        }
      }
    },
      "/events/{id}/with-users": {
        "delete": {
          "description": "Delete an event and its users",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "OK"
            },
            "400": {
              "description": "Bad Request"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      }
    }
  }
  